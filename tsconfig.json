{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "verbatimModuleSyntax": true,
        "paths": {
            "@components/*": [
                "src/components/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ]
        },
        "jsxImportSource": "react"
    }
}